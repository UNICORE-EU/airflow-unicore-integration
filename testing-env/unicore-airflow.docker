FROM apache/airflow:2.8.2-python3.10
USER airflow

ENV SQLALCHEMY_SILENCE_UBER_WARNING=1

RUN pip --version && python -m pip install --upgrade pip
RUN pip install pyunicore

ADD /dist/airflow_unicore_integration-0.0.1.dev0-py3-none-any.whl /airflow_unicore_integration-0.0.1.dev0-py3-none-any.whl
RUN pip install /airflow_unicore_integration-0.0.1.dev0-py3-none-any.whl
